{
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4],
        "no-console": 0,
        "comma-dangle": [2, "never"],
        "react/jsx-indent-props": [2, 4],
        "jsx-quotes": [2, "prefer-single"]
    },
    "root": true,
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    }
}
